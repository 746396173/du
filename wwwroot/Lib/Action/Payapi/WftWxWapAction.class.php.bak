                    $payname = $Order->where("TransID = '".$_TransID."'")->getField("payname");

                    $tranAmt = $Order->where("TransID = '".$_TransID."'")->getField("trademoney");
                    $fhlx = $Order->where("TransID = '".$_TransID."'")->getField("fhlx");
